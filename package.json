{
  "name": "svg-eslint-parser",
  "type": "module",
  "version": "0.0.4",
  "packageManager": "pnpm@10.24.0",
  "description": "An SVG parser that produces output compatible with ESLint.",
  "keywords": [
    "eslint-parser",
    "parser",
    "svg"
  ],
  "license": "MIT",
  "author": {
    "name": "ntnyq",
    "email": "ntnyq13@gmail.com"
  },
  "homepage": "https://github.com/ntnyq/svg-eslint-parser#readme",
  "repository": "ntnyq/svg-eslint-parser",
  "bugs": {
    "url": "https://github.com/ntnyq/svg-eslint-parser/issues"
  },
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "tsdown",
    "deploy": "run-s build docs:build",
    "dev": "tsdown --watch",
    "docs:build": "pnpm -C docs run build",
    "docs:dev": "pnpm -C docs run dev",
    "lint": "eslint",
    "prepare": "husky",
    "prepublishOnly": "pnpm run build",
    "release": "run-s release:check release:version release:publish",
    "release:check": "run-s lint typecheck test",
    "release:publish": "pnpm publish",
    "release:version": "bumpp",
    "test": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@ntnyq/utils": "^0.9.2",
    "eslint-visitor-keys": "^5.0.0"
  },
  "devDependencies": {
    "@ntnyq/eslint-config": "^5.8.0",
    "@ntnyq/prettier-config": "^3.0.1",
    "@types/node": "^24.10.1",
    "bumpp": "^10.3.2",
    "eslint": "^9.39.1",
    "husky": "^9.1.7",
    "nano-staged": "^0.9.0",
    "npm-run-all2": "^8.0.4",
    "prettier": "^3.7.3",
    "tsdown": "^0.16.8",
    "typescript": "^5.9.3",
    "vitest": "^4.0.14"
  },
  "nano-staged": {
    "*.{js,ts,mjs,cjs,vue,md,yml,yaml,json}": "eslint --fix",
    "*.{css,scss,html}": "prettier -uw"
  }
}
