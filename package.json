{
  "name": "svg-eslint-parser",
  "type": "module",
  "version": "0.0.3",
  "packageManager": "pnpm@10.8.0",
  "description": "An SVG parser that produces output compatible with ESLint.",
  "keywords": [
    "eslint-parser",
    "parser",
    "svg"
  ],
  "license": "MIT",
  "author": {
    "name": "ntnyq",
    "email": "ntnyq13@gmail.com"
  },
  "homepage": "https://github.com/ntnyq/svg-eslint-parser#readme",
  "repository": "ntnyq/svg-eslint-parser",
  "bugs": {
    "url": "https://github.com/ntnyq/svg-eslint-parser/issues"
  },
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./dist/index.d.mts",
      "default": "./dist/index.mjs"
    }
  },
  "main": "./dist/index.mjs",
  "types": "./dist/index.d.mts",
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "unbuild",
    "coverage": "vitest --coverage",
    "deploy": "run-s build docs:build",
    "dev": "unbuild --watch",
    "docs:build": "pnpm -C docs run build",
    "docs:dev": "pnpm -C docs run dev",
    "lint": "eslint",
    "prepare": "husky",
    "prepublishOnly": "pnpm run build",
    "release": "run-s release:check release:publish",
    "release:check": "run-s lint typecheck test",
    "release:publish": "bumpp && pnpm publish",
    "test": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@ntnyq/utils": "catalog:",
    "eslint-visitor-keys": "catalog:"
  },
  "devDependencies": {
    "@ntnyq/eslint-config": "catalog:",
    "@ntnyq/prettier-config": "catalog:",
    "@types/node": "catalog:",
    "@vitest/coverage-v8": "catalog:",
    "bumpp": "catalog:",
    "eslint": "catalog:",
    "husky": "catalog:",
    "nano-staged": "catalog:",
    "npm-run-all2": "catalog:",
    "prettier": "catalog:",
    "typescript": "catalog:",
    "unbuild": "catalog:",
    "vitest": "catalog:"
  },
  "engines": {
    "node": ">=18.18.0"
  },
  "nano-staged": {
    "*.{js,ts,mjs,cjs,vue,md,yml,yaml,json}": "eslint --fix",
    "*.{css,scss,html}": "prettier -uw"
  }
}
