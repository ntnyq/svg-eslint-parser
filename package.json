{
  "name": "svg-eslint-parser",
  "type": "module",
  "version": "0.0.2",
  "packageManager": "pnpm@10.2.0",
  "description": "An SVG parser that produces output compatible with ESLint.",
  "keywords": [
    "eslint-parser",
    "parser",
    "svg"
  ],
  "license": "MIT",
  "author": {
    "name": "ntnyq",
    "email": "ntnyq13@gmail.com"
  },
  "homepage": "https://github.com/ntnyq/svg-eslint-parser#readme",
  "repository": "ntnyq/svg-eslint-parser",
  "bugs": {
    "url": "https://github.com/ntnyq/svg-eslint-parser/issues"
  },
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "tsup",
    "coverage": "vitest --coverage",
    "deploy": "run-s build docs:build",
    "dev": "tsup --watch src",
    "docs:build": "pnpm -C docs run build",
    "docs:dev": "pnpm -C docs run dev",
    "lint": "eslint .",
    "prepare": "husky",
    "prepublishOnly": "pnpm run build",
    "release": "run-s release:check release:publish",
    "release:check": "run-s lint typecheck test",
    "release:publish": "bumpp && pnpm publish",
    "test": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@ntnyq/utils": "catalog:",
    "eslint-visitor-keys": "^4.2.0"
  },
  "devDependencies": {
    "@ntnyq/eslint-config": "^3.12.3",
    "@ntnyq/prettier-config": "^1.22.1",
    "@types/node": "^22.13.1",
    "@vitest/coverage-v8": "^3.0.5",
    "bumpp": "^10.0.2",
    "eslint": "^9.19.0",
    "husky": "^9.1.7",
    "nano-staged": "^0.8.0",
    "npm-run-all2": "^7.0.2",
    "prettier": "^3.4.2",
    "tsup": "^8.3.6",
    "typescript": "^5.7.3",
    "vitest": "^3.0.5"
  },
  "engines": {
    "node": ">=18.18.0"
  },
  "pnpm": {
    "patchedDependencies": {
      "vue-resizable-panels": "patches/vue-resizable-panels.patch"
    }
  },
  "nano-staged": {
    "*.{js,ts,mjs,cjs,vue,json,md,yml,yaml}": "eslint --fix"
  }
}
