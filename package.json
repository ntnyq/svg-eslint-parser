{
  "name": "svg-eslint-parser",
  "type": "module",
  "version": "0.0.4",
  "packageManager": "pnpm@10.11.0",
  "description": "An SVG parser that produces output compatible with ESLint.",
  "keywords": [
    "eslint-parser",
    "parser",
    "svg"
  ],
  "license": "MIT",
  "author": {
    "name": "ntnyq",
    "email": "ntnyq13@gmail.com"
  },
  "homepage": "https://github.com/ntnyq/svg-eslint-parser#readme",
  "repository": "ntnyq/svg-eslint-parser",
  "bugs": {
    "url": "https://github.com/ntnyq/svg-eslint-parser/issues"
  },
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./dist/index.d.mts",
      "default": "./dist/index.mjs"
    }
  },
  "main": "./dist/index.mjs",
  "types": "./dist/index.d.mts",
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "unbuild",
    "coverage": "vitest --coverage",
    "deploy": "run-s build docs:build",
    "dev": "unbuild --watch",
    "docs:build": "pnpm -C docs run build",
    "docs:dev": "pnpm -C docs run dev",
    "lint": "eslint",
    "prepare": "husky",
    "prepublishOnly": "pnpm run build",
    "release": "run-s release:check release:version release:publish",
    "release:check": "run-s lint typecheck test",
    "release:publish": "pnpm publish",
    "release:version": "bumpp",
    "test": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@ntnyq/utils": "^0.7.0",
    "eslint-visitor-keys": "^4.2.0"
  },
  "devDependencies": {
    "@ntnyq/eslint-config": "^4.3.0",
    "@ntnyq/prettier-config": "^2.2.0",
    "@types/node": "^22.15.21",
    "@vitest/coverage-v8": "^3.1.4",
    "bumpp": "^10.1.1",
    "eslint": "^9.27.0",
    "husky": "^9.1.7",
    "nano-staged": "^0.8.0",
    "npm-run-all2": "^8.0.4",
    "prettier": "^3.5.3",
    "typescript": "^5.8.3",
    "unbuild": "^3.5.0",
    "vitest": "^3.1.4"
  },
  "engines": {
    "node": ">=18.18.0"
  },
  "nano-staged": {
    "*.{js,ts,mjs,cjs,vue,md,yml,yaml,json}": "eslint --fix",
    "*.{css,scss,html}": "prettier -uw"
  }
}
